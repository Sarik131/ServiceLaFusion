<?xml version="1.0"?>
<resources>
    <!-- Base application theme. -->

    <style parent="Theme.AppCompat.Light.NoActionBar" name="AppTheme">
        <!-- Customize your theme here. -->

        <item name="colorPrimary">#13595e</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="android:colorBackground">@color/colorBackgroundWhite</item>
        <item name="android:windowBackground">@color/colorBackgroundWhite</item>
    </style>

    <style parent="Theme.AppCompat.NoActionBar" name="AppThemeNew">
        <!-- Customize your theme here. -->

        <item name="colorPrimary">#13595E</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="android:colorBackground">@color/colorBackgroundWhite</item>
        <item name="android:windowBackground">@color/colorBackgroundWhite</item>
    </style>
    <style parent="Theme.AppCompat.Light.DarkActionBar" name="Actionbar">
        <item name="colorPrimary">#13595E</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>


    </style>
    <style name="EditTextStyle">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">@dimen/app_normal_margin</item>
        <item name="android:layout_marginRight">@dimen/app_normal_margin</item>
    </style>

    <style name="TextViewStyle">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:textSize">15sp</item>

    </style>

    <!--android:layout_width="match_parent"-->
    <!--android:layout_height="wrap_content"-->
    <!--android:layout_marginTop="@dimen/app_normal_margin"-->
    <!--android:layout_marginBottom="@dimen/app_normal_margin"-->
    <!--android:layout_marginLeft="@dimen/app_normal_margin"-->
    <!--android:layout_marginRight="@dimen/app_normal_margin"-->

    <style name="ButtonStyle">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginRight">@dimen/app_normal_margin</item>
        <item name="android:layout_marginLeft">@dimen/app_normal_margin</item>
        <item name="android:layout_marginTop">@dimen/app_normal_margin</item>
        <item name="android:layout_marginBottom">@dimen/app_normal_margin</item>
        <item name="android:background">@drawable/round_button</item>
        <item name="android:textColor">@android:color/white</item>
        <item name="android:textSize">17sp</item>

    </style>


    <style name="SpinnerStyle">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">@dimen/app_normal_margin</item>
        <item name="android:layout_marginRight">@dimen/app_normal_margin</item>
    </style>


</resources>

